<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#BBB1E6" />
      <stroke
          android:width="1dp"
          android:color="#EFF7DA" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="20dp"
          android:top="26dp"
          android:right="20dp"
          android:bottom="26dp" />
    </shape>
  </item>
  <item>
    <shape>
        <solid
          android:color="#A69DCC" />
      <stroke
          android:width="1dp"
          android:color="#A69DCC" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="20dp"
          android:top="26dp"
          android:right="20dp"
          android:bottom="26dp" />
    </shape>
  </item>
</selector>